{
  "name": "rockbuzz/lara-activities",
  "description": "Lara Activities is a package that monitors activities for creating, editing, and deleting features, including manipulations of related tables.",
  "keywords": [
    "laravel",
    "activities",
    "logs"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "TiagoDevWeb",
      "email": "tiago@rockbuzz.com.br"
    },
    {
      "name": "Paulo Henrique P. Scheuermann",
      "email": "paulo@rockbuzz.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rockbuzz\\LaraActivities\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "fico7489/laravel-pivot": "^3.0",
    "laravel/framework": "^7.0|^8.0|^9.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "orchestra/testbench": "^5.0",
    "squizlabs/php_codesniffer": "^3.5"
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "cs": "vendor/bin/phpcs"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Rockbuzz\\LaraActivities\\ServiceProvider"
      ]
    }
  }
}
